rbbt-phgx=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin {
 }
 lib=lib{
  phgx.rb
  sources=rbbt/sources {
   cancer.rb
   matador.rb
   kegg.rb
   reactome.rb
   pharmagkb.rb
   stitch.rb
   string.rb
   hprd.rb
   dbsnp.rb
   pina.rb
  }
  mutation=rbbt/mutation{
   snps_and_go.rb
   fireDB.rb
   sift.rb
   polyphen.rb
   mutation_assessor.rb
   transFIC.rb
   chasm.rb
  }
 }
 test=test {
  test_helper.rb
  sources=rbbt/sources {
   test_cancer.rb
   test_matador.rb
   test_pharmagkb.rb
   test_stitch.rb
  }
  mutation=rbbt/mutation{
   test_fireDB.rb
   test_polyphen.rb
   test_sift.rb
   test_snps_and_go.rb
  }
 }
 share=share{
  install=install{
   software=software{
    OncodriveFM
   }
   lib=lib{
    rake_helper.rb
   }
   Matador=Matador{
    Rakefile
   }
   KEGG=KEGG{
    Rakefile
   }
   PharmaGKB=PharmaGKB{
    Rakefile
   }
   STITCH=STITCH{
    Rakefile
   }
   STRING=STRING{
    Rakefile
   }
   NCI=NCI{
    Rakefile
   }
   Reactome=Reactome{
    Rakefile
   }
   HPRD=HPRD{
    Rakefile
   }
   Pina=Pina{
    Rakefile
   }
  }
  Cancer=Cancer filter="*"{
   anais_annotations
   anais_interactions
   cancer_genes.tsv
  }
 }
}
