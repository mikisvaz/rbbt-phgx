rbbt-phgx=/$PWD filter="*.rb *.rake Rakefile *.rdoc *.R *.sh *.js *.haml *.sass *.txt *.conf" {
 Rakefile
 bin=bin {
 }
 lib=lib{
  phgx.rb
  sources=rbbt/sources {
   matador.rb
   pharmagkb.rb
   stitch.rb
   string.rb
  }
 }
 test=test {
  test_helper.rb
  sources=rbbt/sources {
   test_matador.rb
   test_pharmagkb.rb
   test_stitch.rb
  }
 }
 share=share{
  install=install{
   lib=lib{
    rake_helper.rb
   }
   Matador=Matador{
    Rakefile
    }
   PharmaGKB=PharmaGKB{
    Rakefile
   }
   STITCH=STITCH{
    Rakefile
   }
  }
 }
}
